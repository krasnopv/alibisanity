{
  "name": "alibi",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "dev:staging": "cross-env SANITY_DATASET=staging SANITY_STUDIO_TITLE='Alibi (Staging)' sanity dev",
    "dev:production": "cross-env SANITY_DATASET=production SANITY_STUDIO_TITLE='Alibi (Production)' sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "build:staging": "cross-env SANITY_DATASET=staging SANITY_STUDIO_TITLE='Alibi (Staging)' sanity build",
    "build:production": "cross-env SANITY_DATASET=production SANITY_STUDIO_TITLE='Alibi (Production)' sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "docker:build:staging": "docker build -f Dockerfile.staging -t alibi-studio-staging .",
    "docker:up:staging": "docker-compose -f docker-compose.staging.yml up -d",
    "docker:down:staging": "docker-compose -f docker-compose.staging.yml down",
    "migrate:to-staging": "node scripts/migrate-to-staging.js"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/vision": "^4.10.1",
    "lucide-react": "^0.544.0",
    "react": "^19.1",
    "react-dom": "^19.1",
    "sanity": "^4.11.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/react": "^19.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.28",
    "prettier": "^3.5",
    "typescript": "^5.8"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
}
